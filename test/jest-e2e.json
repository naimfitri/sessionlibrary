{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@app/audit(|/.*)$": "<rootDir>/../libs/audit/src/$1",
    "^@app/log(|/.*)$": "<rootDir>/../libs/log/src/$1"
  }
}